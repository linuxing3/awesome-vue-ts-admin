    "border-radius-base": "3px",
    "border-radius-sm": "2px",
    "shadow-color": "rgba(0,0,0,0.05)",
    "shadow-1-down": "4px 4px 40px @shadow-color",
    "border-color-split": "#f4f4f4",
    "border-color-base": "#e5e5e5",
    "font-size-base": "13px",
    "text-color": "#666"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx,ts,tsx,vue}": [
        "yarn lint --fix",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/*",
      "**/dist_electron/*",
      "**/node_modules/**/*"
    ]
  }
}
