    "@types/chart.js": "^2.7.50",
    "@types/jest": "^23.1.4",
    "@types/js-cookie": "^2.2.2",
    "@types/jsonp": "^0.2.0",
    "@types/lodash": "^4.14.123",
    "@types/qs": "^6.5.2",
