{
  "name": "awesome-vue-ts-admin",
  "version": "1.0.0",
  "private": true,
  "description": "A awesome admin system powered by vue and electron",
  "author": "linuxing3",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "gen": "hygen gen",
    "dev": "vue-cli-service electron:serve",
    "dist": "cross-env NODE_ENV=production vue-cli-service electron:build",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:debug": "electron --inspect=5858 dist_electron",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "apollo:serve": "ts-node src/playpage/server/apolloServer.ts",
    "test:e2e": "vue-cli-service test:e2e",
    "inspect": "vue inspect > ./webpack.config.inspect.js"
  },
  "main": "background.js",
  "dependencies": {
