    "apollo-client": "^2.6.2",
    "apollo-link": "^1.2.11",
    "apollo-link-http": "^1.5.14",
    "axios": "^0.18.0",
    "babel-plugin-import": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chart.js": "^2.8.0",
    "constants": "^0.0.2",
    "cookies": "^0.7.3",
    "csv": "^5.1.1",
    "docx": "^4.7.0",
    "docx-template": "^2.2.3",
    "docx-templates": "^3.0.0",
    "electron": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "graphql": "^14.3.1",
    "graphql-tag": "^2.10.1",
    "js-cookie": "^2.2.0",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.11",
    "lowdb": "^1.0.0",
    "md5": "^2.2.1",
    "mock": "^0.1.1",
    "mockjs": "^1.0.1-beta3",
    "moment": "^2.24.0",
    "papaparse": "^4.6.3",
    "path-to-regexp": "^3.0.0",
    "q": "^1.5.1",
