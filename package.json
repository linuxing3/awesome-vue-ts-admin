    "electron-icon-builder": "^1.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.2",
    "husky": "^1.3.1",
    "hygen": "^2.1.2",
    "json-format": "^1.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "md5": "^2.2.1",
